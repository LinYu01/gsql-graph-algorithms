CREATE QUERY random_walk(INT batches, INT walk_length = 20, INT num_walks = 4, FLOAT p = 1, FLOAT q = 1, STRING filepath = "/home/tigergraph/path.csv", SET<STRING> edge_types) FOR GRAPH Blog { 
    FILE f (filepath);
    Start = {ANY};
    FOREACH i IN RANGE [0, batches-1] DO
        Start = SELECT s
                FROM Start:s
                WHERE getvid(s) % batches == i;
                POST-ACCUM random_walk_sub(s, walk_length, num_walks, p, q, f, edge_types);
    END;
}